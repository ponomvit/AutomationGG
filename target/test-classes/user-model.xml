<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="userData01" class="springConstructors.UserData">
        <property name="title" value="Mr"/>
        <property name="gender" value="M"/>
        <property name="firstName" value="Player"/>
        <property name="postCode" value="12345"/>
    </bean>

    <bean id="test" class="springConstructors.UserData">
        <property name="title" value="Mr"/>
        <property name="gender" value="M"/>
        <property name="firstName" value="vitalik"/>
        <property name="postCode" value="12345"/>
    </bean>
</beans>